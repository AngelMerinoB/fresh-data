{
	"name": "fresh-data",
	"version": "0.1.0",
	"description": "Describe the data you need and simply use it in your app.",
	"main": "dist/fresh-data.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/coderkevin/fresh-data.git"
	},
	"license": "GPL-2.0+",
	"authors": [
		"Kevin Killingsworth <kevin.killingsworth@automattic.com> (https://github.com/coderkevin)"
	],
	"homepage": "https://github.com/coderkevin/fresh-data#readme",
	"bugs": {
		"url": "https://github.com/coderkevin/fresh-data/issues"
	},
	"keywords": [
		"javascript",
		"react",
		"redux",
		"api",
		"data"
	],
	"browserlist": [
		"last 2 versions",
		"Safari >= 10",
		"iOS >= 10",
		"not ie <= 10",
		"> 1%"
	],
	"engines": {
		"node": "8.9.4",
		"npm": "5.6.0"
	},
	"babel": {
		"presets": [
			"env",
			"react",
			"stage-2"
		],
		"plugins": [
			"add-module-exports"
		]
	},
	"jest": {
		"verbose": true,
		"moduleNameMapper": {
			"\\.(css)$": "identity-obj-proxy"
		},
		"setupTestFrameworkScriptFile": "jest-enzyme",
		"testEnvironment": "enzyme",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/dist/",
			"/examples/"
		]
	},
	"scripts": {
		"lint": "eslint .",
		"test": "cross-env BABEL_ENV=test NODE_ENV=test jest --runInBand --coverage",
		"test:watch": "npm test -- --watch",
		"dev": "webpack --mode development",
		"dev:watch": "webpack --mode development --watch",
		"build": "webpack --mode production"
	},
	"devDependencies": {
		"babel-eslint": "^8.2.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"codecov": "^3.0.2",
		"cross-env": "^5.1.6",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^4.19.1",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-plugin-jest": "^21.15.2",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-wordpress": "git://github.com/WordPress-Coding-Standards/eslint-plugin-wordpress.git",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^22.4.3",
		"jest-environment-enzyme": "^6.0.1",
		"jest-enzyme": "^6.0.1",
		"react-redux": "^5.0.7",
		"redux": "^3.7.2",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.1.4",
		"webpack-dev-server": "^3.1.4"
	},
	"dependencies": {
		"debug": "^3.1.0",
		"lodash": "^4.17.10",
		"react": "^16.4.0",
		"react-dom": "^16.4.0"
	}
}
